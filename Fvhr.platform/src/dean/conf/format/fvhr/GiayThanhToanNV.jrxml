	<queryString>
		<![CDATA[Select a.empsn, a.empcn as IC, a.fname ||' '|| a.lname as fname, a.lname, a.date_hired as NNX, a.position as possn ,b.name_fact||' '|| b.name_group as fgroup
, b.name_dept_name as donvi, c.bsalary, d.qdnv
,substr(to_char(d.real_off_date,'dd/mm/yyyy'), 1, 2) as n_nghi
,substr(to_char(d.real_off_date,'dd/mm/yyyy'), 4, 2) as t_nghi
,substr(to_char(d.real_off_date,'dd/mm/yyyy'), 7, 4) as na_nghi
, d.id_quit_reason, d.real_off_date
,Trunc((d.real_off_date - a.date_hired) / 365, 0) as nam1, f.note1
,Trunc(((d.real_off_date - a.date_hired) - trunc((d.real_off_date - a.date_hired) / 365, 0) * 365) / 30, 0) as thang1

,   Trunc ((d.real_off_date-a.date_hired)/365,0)*12
   + trunc(trunc ( (d.real_off_date-a.date_hired)/365,0) / 5 ,0)
   +
  (
  (case
   When d.real_off_date>=to_date('15/'||to_char(d.real_off_date,'mm/yyyy'),'dd/mm/yyyy') then
   Trunc(((d.real_off_date - a.date_hired)  - trunc((d.real_off_date - a.date_hired) / 365, 0) * 365) / 30,0)+1
   When d.real_off_date<to_date('15/'||to_char(d.real_off_date,'mm/yyyy'),'dd/mm/yyyy') then
   Trunc(((d.real_off_date - a.date_hired)  - trunc((d.real_off_date - a.date_hired) / 365, 0) * 365) / 30,0)
   end)
   ) as OBTAIN

,    (case
   When d.real_off_date>=to_date('15/'||to_char(d.real_off_date,'mm/yyyy'),'dd/mm/yyyy') then
   To_number(to_char(d.real_off_date,'mm'))
   When d.real_off_date<to_date('15/'||to_char(d.real_off_date,'mm/yyyy'),'dd/mm/yyyy') then
   To_number(to_char(d.real_off_date,'mm'))-1
   end) as TS_HTAI

   , nvl((select e.used from n_rest e
   where e.empsn = a.empsn and e.year = to_char(d.real_off_date,'yyyy')),0) as TS_USED_HTAI

  ,(case
  When trunc((trunc((d.real_off_date - a.date_hired) / 365, 0) * 365) / 30, 0) >= 6 then
  (case
  When (d.real_off_date >= to_date('31/12/2008','dd/mm/yyyy')) then
  Trunc((to_date('31/12/2008','dd/mm/yyyy') - a.date_hired) / 365, 0) * 0.5
  When (d.real_off_date <to_date('31/12/2008','dd/mm/yyyy')) then
  Trunc((d.real_off_date - a.date_hired) / 365, 0) * 0.5
   end) + 0.25
  When trunc((trunc((d.real_off_date - a.date_hired) / 365, 0) * 365) / 30,0) < 6 then
   (case
  When (d.real_off_date >=to_date('31/12/2008','dd/mm/yyyy')) then
  Trunc((to_date('31/12/2008','dd/mm/yyyy') - a.date_hired) / 365,0) * 0.5
  When (d.real_off_date <to_date('31/12/2008','dd/mm/yyyy')) then
  Trunc((d.real_off_date - a.date_hired) / 365, 0) * 0.5
   end)
   end) as trocap1

,(select count(*) from n_rest_detail f
where f.empsn=a.empsn
and to_char(date_off,'yyyy')=to_char(d.real_off_date,'yyyy') and rest_type='TON') TON_USED_HTAI

,nvl((select e.stored from n_rest e
where e.empsn=a.empsn
and e.year=to_char(d.real_off_date,'yyyy')),0) TON_HTAI


  From n_employee a, n_department b, n_basic_salary c, n_emp_quit d, N_Quit_Reason f
  Where a.depsn = b.id_dept and a.empsn = c.empsn and a.empsn = d.empsn and d.id_quit_reason = f.id_quit_reason
  and c.keys = 1
and a.empsn in ('00080510','06120005','00070164','07091263'
,'08080462','00070164','10091020','10110302','10031126')]]>
	</queryString>
